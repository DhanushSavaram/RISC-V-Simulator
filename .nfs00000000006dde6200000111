    li t0, 5               # Load immediate value 5 into t0
    slti t1, t0, 10        # t1 = (t0 < 10) ? 1 : 0, expected 1
    slti t2, t0, 5         # t2 = (t0 < 5) ? 1 : 0, expected 0
    slti t3, t0, -10       # t3 = (t0 < -10) ? 1 : 0, expected 0
    jr ra                  # Return execution
